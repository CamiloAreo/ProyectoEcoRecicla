<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:tools="http://schemas.android.com/tools">

    <RelativeLayout
        android:id="@+id/InicioDeSesion"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#917D65"
        tools:layout_editor_absoluteX="68dp"
        tools:layout_editor_absoluteY="67dp">

        <View
            android:id="@+id/EcoReciclalogo_img"
            android:layout_width="100dp"
            android:layout_height="100dp"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:layout_marginLeft="28dp"
            android:layout_marginTop="55dp"
            android:background="@drawable/ecoreciclalogo" />

        <View
            android:id="@+id/EcoReciclalogo_lbl"
            android:layout_width="238dp"
            android:layout_height="42dp"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:layout_marginLeft="141dp"
            android:layout_marginTop="86dp"
            android:background="@drawable/ecoreciclamarca" />

        <View
            android:id="@+id/estadisticasTitulo_img"
            android:layout_width="351dp"
            android:layout_height="58dp"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:layout_marginLeft="27dp"
            android:layout_marginTop="180dp"
            android:background="@drawable/estadisticas_titulo" />

        <TextView
            android:id="@+id/historial_lbl"
            android:layout_width="319dp"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:layout_alignParentBottom="true"
            android:layout_marginStart="41dp"
            android:layout_marginBottom="574dp"
            android:text="Cantidad en kilos reciclados del ultimo mes"
            android:textAlignment="center"
            android:textColor="@color/white"
            android:textSize="24sp" />

        <View
            android:id="@+id/grafica_img"
            android:layout_width="323dp"
            android:layout_height="213dp"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:layout_marginLeft="40dp"
            android:layout_marginTop="336dp"
            android:background="@drawable/estadisticas_grafica" />

        <TextView
            android:id="@+id/volver_lbl"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:layout_alignParentBottom="true"
            android:layout_marginStart="129dp"
            android:layout_marginBottom="53dp"
            android:text="Volver"
            android:textColor="@color/white"
            android:textSize="26sp" />


        <Button
            android:id="@+id/filtro_btn"
            android:layout_width="74dp"
            android:layout_height="74dp"
            android:layout_alignParentEnd="true"
            android:layout_alignParentBottom="true"
            android:layout_marginEnd="289dp"
            android:layout_marginBottom="256dp"
            android:backgroundTint="#149832"
            app:cornerRadius="20dp" />

        <View
            android:id="@+id/filtro_img"
            android:layout_width="46dp"
            android:layout_height="46dp"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:layout_marginLeft="63dp"
            android:layout_marginTop="575dp"
            android:background="@drawable/filtro_logo" />

        <TextView
            android:id="@+id/filtro_lbl"
            android:layout_width="226dp"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:layout_alignParentBottom="true"
            android:layout_marginStart="135dp"
            android:layout_marginBottom="277dp"
            android:text="Filtrar por Categoría"
            android:textColor="@color/white"
            android:textSize="24sp" />

        <Button
            android:id="@+id/ajustar_btn"
            android:layout_width="74dp"
            android:layout_height="74dp"
            android:layout_alignParentEnd="true"
            android:layout_alignParentBottom="true"
            android:layout_marginEnd="289dp"
            android:layout_marginBottom="168dp"
            android:backgroundTint="#149832"
            app:cornerRadius="20dp" />

        <View
            android:id="@+id/ajustar_img"
            android:layout_width="46dp"
            android:layout_height="46dp"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:layout_marginLeft="62dp"
            android:layout_marginTop="664dp"
            android:background="@drawable/ajustes_logo" />

        <TextView
            android:id="@+id/ajustar_lbl"
            android:layout_width="226dp"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:layout_alignParentBottom="true"
            android:layout_marginStart="135dp"
            android:layout_marginBottom="177dp"
            android:text="Ajustar rango de fechas en la gráfica"
            android:textColor="@color/white"
            android:textSize="24sp" />

        <Button
            android:id="@+id/volver_btn"
            android:layout_width="74dp"
            android:layout_height="74dp"
            android:layout_alignParentEnd="true"
            android:layout_alignParentBottom="true"
            android:layout_marginEnd="294dp"
            android:layout_marginBottom="34dp"
            android:backgroundTint="#149832"
            app:cornerRadius="20dp" />

        <View
            android:id="@+id/volver_img"
            android:layout_width="46dp"
            android:layout_height="46dp"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:layout_marginLeft="57dp"
            android:layout_marginTop="797dp"
            android:background="@drawable/volver_logo" />

    </RelativeLayout>

</androidx.constraintlayout.widget.ConstraintLayout>